  "author": "reconbot",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@microsoft/api-extractor": "^6.3.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.4",
    "benchmark": "^2.1.4",
    "bluestream": "^10.3.3",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.2",
    "rollup": "^1.4.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "through2-concurrent": "^2.0.0",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3333"
  },
  "engines": {
    "node": ">=8"
  }
